<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="CENTER" prefHeight="58.0" prefWidth="391.0" spacing="10.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="avatarimgview" fitHeight="37.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../assets/avatar.png" />
         </image>
      </ImageView>
      <Label fx:id="namelabel" prefHeight="20.0" prefWidth="122.0" text="NAME" textFill="#2a1e17">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <AnchorPane prefHeight="58.0" prefWidth="82.0" />
      <Label fx:id="pointslabel" prefHeight="20.0" prefWidth="68.0" text="Points" textFill="#eee513">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
   <stylesheets>
      <URL value="@../css/button.css" />
      <URL value="@../css/label.css" />
   </stylesheets>
</HBox>
